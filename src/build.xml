<project name="Aufgabe1" default="compile" basedir=".">
	<!-- This is a very simple ANT build file for compilation on the OOP server -->
	
	<description>OOP_WS2012_Gruppe_169</description>

	<property name="src" location="." />
	<property name="build" location="." />

	<target name="init">

	</target>

	<target name="compile" depends="init" description="compile the source ">
		<javac srcdir="${src}" destdir="${build}" />
	</target>

	<target name="clean" description="clean up">
		<delete>
			<fileset dir="." includes="*.class" />
			<fileset dir="./UE1" includes="*.class" />
		</delete>
	</target>
</project>